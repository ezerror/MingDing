<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--AutoFillEmailEditText-->
    <declare-styleable name="AutoFillEmailEditText">
        <attr name="AutoFillEmailEditText_domains" format="string" />
        <attr name="AutoFillEmailEditText_default_drop_down_key_color" format="color" />
        <attr name="AutoFillEmailEditText_default_drop_down_bg" format="reference" />
        <attr name="AutoFillEmailEditText_default_drop_down_divider" format="boolean" />
    </declare-styleable>


    <declare-styleable name="RoundImageView">
        <attr name="type" />
        <attr name="scale" />
        <attr name="borderRadius" />
        <attr name="top_left" />
        <attr name="top_right" />
        <attr name="bottom_left" />
        <attr name="bottom_right" />
    </declare-styleable>


    <declare-styleable name="CountdownView">
        <attr name="isDrawDial" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RatioImageView">
        <attr name="ratio" format="float"/>
    </declare-styleable>

    <declare-styleable name="NineGridLayout">
        <attr name="sapcing" format="dimension"/>
        <attr name="oneImageWidth" format="dimension"/>
        <attr name="oneImageHeight" format="dimension"/>
        <attr name="image_ratio" format="float"/>
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>
    <attr name="scale">
        <enum name="crop" value="0" />
        <enum name="fit" value="1" />
    </attr>

    <attr name="top_left" format="boolean" />
    <attr name="top_right" format="boolean" />
    <attr name="bottom_left" format="boolean" />
    <attr name="bottom_right" format="boolean" />

</resources>


